{
  "name": "igrep-cashbook",
  "type": "module",
  "private": true,
  "version": "0.1.0",
  "description": "Calculates the balance of a cashbook formatted by my original format.",
  "scripts": {
    "custard-transpile": "node ../../packages/command/dist/custard.js transpile -vp .provided-symbols.cstd \"src/*.cstd\"",
    "custard-run": "node ../../packages/command/dist/custard.js run -p .provided-symbols.cstd",
    "custard-transpile:not-working-on-windows": "custard tranpile -vp .provided-symbols-config.cstd src/*.cstd",
    "custard-run:not-working-on-windows": "custard run -p .provided-symbols-config.cstd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igrep/custard.git"
  },
  "author": "YAMAMOTO Yuji",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/igrep/custard/issues"
  },
  "homepage": "https://github.com/igrep/custard#readme",
  "dependencies": {
    "@custard-lang/processor": "^0.0.2"
  },
  "devDependencies": {
    "@custard-lang/command": "^0.0.2"
  }
}
